{
  "name": "ng2-dataobjects",
  "version": "0.0.1",
  "description": "Angular2 data objects",
  "main": "./dist/ng2dataobjects.js",
  "scripts": {
    "test": "npm run build; karma start",
    "docs": "typedoc --out docs/api/ --module commonjs --mode modules --name ng2dataobjects src",
    "build": "rm -rf dist; tsc",
    "build_npm": "rm -rf dist && tsc -p tsconfig.publish.es5.json && tsc -p tsconfig.publish.es6.json && tsc -p tsconfig.publish.es6-deprecated.json",
    "postbuild_npm": "cp package.json README.md .npmignore dist/",
    "build_bundle": "cp -r src ng2dataobjects && tsc typings/main.d.ts ng2dataobjects.ts --rootDir . --module system -t es5 --outFile dist/bundles/ng2dataobjects.js  --moduleResolution node --emitDecoratorMetadata --experimentalDecorators"
  },
  "author": "Jean-Matthieu BARBIER <jm.barbier@solidev.net>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/http": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "lodash": "^4.13.1",
    "rxjs": "^5.0.0-beta.6"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "es6-module-loader": "^0.17.10",
    "es6-shim": "^0.35.0",
    "http-server": "^0.8.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json": "^9.0.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^2.0.2",
    "karma-systemjs": "^0.10.0",
    "parse5": "^1.3.2",
    "protractor": "3.0.0",
    "reflect-metadata": "0.1.2",
    "systemjs": "^0.19.16",
    "systemjs-builder": "^0.15.7",
    "traceur": "0.0.96",
    "typedoc": "github:jeffbcross/typedoc",
    "typescript": "^1.8.10",
    "typings": "^0.6.2",
    "zone.js": "^0.6.6"
  }
}
