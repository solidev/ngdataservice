{
  "name": "ng2-dataobjects",
  "version": "0.0.0-alpha1",
  "description": "Angular2 data objects",
  "main": "./dist/index.js",
  "scripts": {
    "test": "karma start",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage:reporttxt && npm run coverage:reporthtml && npm run coverage:remap",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,vendor.ts",
    "coverage:reporthtml": "remap-istanbul -i coverage/coverage.json -o coverage/html -t html -e node_modules,vendor.ts",
    "coverage:reporttxt": "remap-istanbul -i coverage/coverage.json -t text-summary -o coverage/summary.txt -e node_modules,vendor.ts && cat coverage/summary.txt",
    "docs": "typedoc --out doc/api/ --module commonjs --mode modules --name ng2dataobjects src",
    "build": "rm -rf dist; tsc -p ./tsconfig.es5.json",
    "build_npm": "rm -rf dist && tsc -p tsconfig.publish.es5.json && tsc -p tsconfig.publish.es6.json && tsc -p tsconfig.publish.es6-deprecated.json",
    "postbuild_npm": "cp package.json README.md .npmignore dist/",
    "build_bundle": "cp -r src ng2dataobjects && tsc typings/main.d.ts ng2dataobjects.ts --rootDir . --module system -t es5 --outFile dist/bundles/ng2dataobjects.js  --moduleResolution node --emitDecoratorMetadata --experimentalDecorators"
  },
  "author": "Jean-Matthieu BARBIER <jm.barbier@solidev.net>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/http": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "lodash": "^4.15.0",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6"
  },
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/core-js": "^0.9.30",
    "@types/lodash": "^4.14.30",
    "@types/mocha": "^2.2.31",
    "@types/node": "^6.0.33",
    "@types/protractor": "^1.5.16",
    "@types/selenium-webdriver": "^2.44.26",
    "@types/sinon": "^1.16.29",
    "chai": "^3.5.0",
    "codelyzer": "0.0.28",
    "conventional-changelog-cli": "^1.2.0",
    "es6-module-loader": "^0.17.11",
    "es6-shim": "^0.35.1",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json": "^9.0.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "parse5": "^2.2.0",
    "protractor": "4.0.3",
    "reflect-metadata": "0.1.8",
    "remap-istanbul": "^0.6.4",
    "sinon": "^2.0.0-pre.2",
    "systemjs": "^0.19.36",
    "systemjs-builder": "^0.15.26",
    "traceur": "0.0.111",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.14.0",
    "typedoc": "github:jeffbcross/typedoc",
    "typescript": "^2.0.0-beta",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "zone.js": "^0.6.12"
  }
}
